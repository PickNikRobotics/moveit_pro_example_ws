        ID="BreakpointSubscriber"
        breakpoint_topic="/moveit_pro_breakpoint"
      />
      <SubTree
        ID="Move to Pose"
        _collapsed="false"
        target_pose="{target_pose}"
      />
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Move To Loaded Pose">
      <MetadataFields>
        <Metadata subcategory="Motion - Execute" />
        <Metadata runnable="true" />
      </MetadataFields>
    </SubTree>
  </TreeNodesModel>
</root>
