  <BehaviorTree
    ID="Move To Loaded Pose"
    _description=""
    _favorite="false"
    _subtreeOnly="false"
  >
    <Control ID="Sequence" name="TopLevelSequence">
      <Action
        ID="LoadPoseStampedFromYaml"
        output="{target_pose}"
        file_path="approach_pose.yaml"
      />
