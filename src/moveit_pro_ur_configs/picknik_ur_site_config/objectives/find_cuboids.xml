<?xml version='1.0' encoding='utf-8'?>
<root BTCPP_format="4" main_tree_to_execute="Find Cuboids">
  <BehaviorTree ID="Find Cuboids" _description="Detect and move to cuboid object in current scene">
    <Control ID="Sequence" name="root">
      <Action ID="LoadObjectiveParameters" config_file_name="find_cuboids_config.yaml" parameters="{parameters}"/>
      <Action ID="GetPointCloud" topic_name="/wrist_mounted_camera/depth/color/points" message_out="{point_cloud}"/>
      <Action ID="FindSingularCuboids" point_cloud="{point_cloud}" parameters="{parameters}"/>
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Find Cuboids">
      <MetadataFields>
        <Metadata subcategory="Application - Basic Examples"/>
        <Metadata runnable="true"/>
      </MetadataFields>
    </SubTree>
  </TreeNodesModel>
</root>
