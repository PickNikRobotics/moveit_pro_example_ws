<?xml version="1.0" encoding="UTF-8" ?>
<root BTCPP_format="4" main_tree_to_execute="Cycle Between Poses">
  <!--//////////-->
  <BehaviorTree
    ID="Cycle Between Poses"
    _description="Simple example objective for multi arm motions. Two arms are moved to different waypoints."
    _favorite="true"
    _hardcoded="false"
  >
    <Decorator ID="KeepRunningUntilFailure">
      <Control ID="Sequence">
        <SubTree
          ID="Move to Waypoint"
          _collapsed="true"
          joint_group_name="manipulator"
          link_padding="0.0"
          waypoint_name="Home"
          target_joint_state="{target_joint_state}"
        />
        <SubTree
          ID="Move to Waypoint"
          _collapsed="true"
          joint_group_name="manipulator"
          link_padding="0.0"
          waypoint_name="Flip"
          target_joint_state="{target_joint_state}"
        />
      </Control>
    </Decorator>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Cycle Between Poses">
      <MetadataFields>
        <Metadata subcategory="Motion - Execute" />
      </MetadataFields>
    </SubTree>
  </TreeNodesModel>
</root>
