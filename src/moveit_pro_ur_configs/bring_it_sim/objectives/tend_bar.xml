<?xml version="1.0" encoding="utf-8" ?>
<root BTCPP_format="4" main_tree_to_execute="Tend Bar">
  <!--//////////-->
  <BehaviorTree
    ID="Tend Bar"
    _description="Navigates the mobile base back to behind the bar"
    _favorite="false"
  >
    <Control ID="Sequence" name="TopLevelSequence">
      <Action
        ID="CreateStampedPose"
        orientation_xyzw="0;0;0;1"
        position_xyz="0;0;0"
        stamped_pose="{home_location}"
      />
      <Action
        ID="NavigateToPoseAction"
        pose_stamped="{home_location}"
        behavior_tree_path="/opt/ros/humble/share/nav2_bt_navigator/behavior_trees/navigate_to_pose_w_replanning_and_recovery.xml"
        action_name="/navigate_to_pose"
      />
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Tend Bar" />
  </TreeNodesModel>
</root>
