        <!-- <box size="${x_length} ${side_wall_thickness} ${z_length}"/> -->
        <!-- <origin xyz="${x_length/2} ${y_length - side_wall_thickness/2} ${z_length/2}"/> -->
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 -0.178 0.09775" rgba=".5 .5 .5 1"/>
      </body>

      <body name="right_bin" pos="0.25 0 0.025" quat="0.707 0 0 0.707">
        <geom type="box" size="0.3048 0.1905 0.0025" pos="0 0 0.0025" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.0125 0.190 0.09525" pos="0.2923 0 0.09775" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.0125 0.190 0.09525" pos="-0.2923 0 0.09775" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 0.178 0.09775" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 -0.178 0.09775" rgba=".5 .5 .5 1"/>
      </body>
    </body>

    
    <!-- 4 7 2 -->
    <!-- 3 6 2 -->
    <replicate count="3" sep="-" offset=".07 0 0">
      <replicate count="6" sep="-" offset="0 .07 0">
        <replicate count="2" sep="-" offset="0 0 0.5">
          <body name="sphere-0" pos="-0.35 0.4 0.5">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="box" size="0.025 0.025 0.025" condim="1" priority="1" rgba="0.8 0.2 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- 3 6 2 -->
    <replicate count="3" sep="-" offset=".07 0 0">
      <replicate count="6" sep="-" offset="0 .07 0">
        <replicate count="2" sep="-" offset="0 0 0.5">
          <body name="sphere-1" pos="-0.3 0.45 0.75">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="box" size="0.025 0.025 0.025" condim="1" priority="1" rgba="0.8 0.2 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

