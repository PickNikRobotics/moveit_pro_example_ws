    <geom type="box" pos="0.8 0 .8" size=".05 .4 0.8" />

    <!-- Add a scene camera -->
    <camera
      name="scene_camera"
      pos="0.0 1.5 2.6"
      fovy="58"
      mode="fixed"
      resolution="640 480"
      xyaxes="-1.000 0.000 -0.000 -0.000 -0.878 0.479"
    />
    <site
      name="scene_camera_optical_frame"
      pos="0.0 1.5 2.6"
      xyaxes="-1.000 0.000 -0.000 0.000 0.878 -0.479"
    />


  </worldbody>

