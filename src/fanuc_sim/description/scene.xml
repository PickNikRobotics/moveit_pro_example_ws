<!-- UR5e inspired from https://github.com/google-deepmind/mujoco_menagerie/tree/main/universal_robots_ur5e -->
<mujoco model="fanuc scene">
  <compiler angle="radian" autolimits="true" />
  <option integrator="RK4" timestep="0.005" />
  <include file="lrmate200id/lrmate200id_globals.xml" />
  <include file="tool.xml" />

  <asset>
    <!-- Define textures and materials -->
    <texture name="wood" type="cube" file="assets/wood.png" />
    <material name="wood" texture="wood" texrepeat="1 1" />

    <!-- Define AprilTag textures -->
    <texture name="apriltag1" type="2d" file="assets/tag36_11_00000.png" />
    <texture name="apriltag2" type="2d" file="assets/tag36_11_00001.png" />

    <!-- Define block materials with AprilTags -->
    <material
      name="apriltag_material1"
      texture="apriltag1"
      texuniform="false"
    />
    <material
      name="apriltag_material2"
      texture="apriltag2"
      texuniform="false"
    />
  </asset>

  <statistic center="0.3 0 0.4" extent="0.8" />

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0" />
    <rgba haze="0.15 0.25 0.35 1" />
    <global azimuth="120" elevation="-20" />
  </visual>

  <asset>
    <texture
      type="skybox"
      builtin="gradient"
      rgb1="0.3 0.5 0.7"
      rgb2="0 0 0"
      width="512"
      height="3072"
    />
    <texture
      type="2d"
      name="groundplane"
      builtin="checker"
      mark="edge"
      rgb1="0.2 0.3 0.4"
      rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8"
      width="300"
      height="300"
    />
    <material
      name="groundplane"
      texture="groundplane"
      texuniform="true"
      texrepeat="5 5"
      reflectance="0.2"
    />
  </asset>
  <asset>
    <mesh name="cube" file="assets/Cube.stl" />
  </asset>

  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" directional="true" />
    <geom
      name="floor"
      pos="0 0 0"
      size="0 0 0.05"
      type="plane"
      material="groundplane"
    />
    <!-- Define the table -->
    <body name="table" pos="0 1 0.2">
      <geom type="box" size="0.5 0.5 0.025" material="wood" />

      <!-- https://github.com/PickNikRobotics/moveit_studio_ur_ws/blob/pr-add-bin-picking-reference-app-2/src/picknik_ur_epick_config/description/environment.xacro -->
      <!-- <xacro:property name="bin_x" value="${24 * 0.0254}" />
      <xacro:property name="bin_y" value="${15 * 0.0254}" />
      <xacro:property name="bin_z" value="${7.5 * 0.0254}" />
      <xacro:property name="bin_wall" value="0.025" />
      <xacro:property name="bin_bottom" value="0.005" /> -->

      <body name="left_bin" pos="-0.25 0 0.025" quat="0.707 0 0 0.707">
        <!-- <geom type="box" size="1 1 .01" pos="0.5 0.5 0" rgba=".5 .5 .5 1"/>
        <geom type="box" size="1 1 .01" pos="0 0 0" rgba=".5 0 0 1"/> -->
        <!-- Bottom -->
        <!-- <box size="${x_length} ${y_length} ${bottom_thickness}"/> -->
        <!-- <origin xyz="${x_length/2} ${y_length/2} ${bottom_thickness/2}"/> -->
        <geom type="box" size="0.3048 0.1905 0.0025" pos="0 0 0.0025" rgba=".5 .5 .5 1"/>
        <!-- Back -->
        <!-- <box size="${side_wall_thickness} ${y_length} ${z_length}"/> -->
        <!-- <origin xyz="${side_wall_thickness/2} ${y_length/2} ${z_length/2}"/> -->
        <!-- <geom type="box" size="0.0125 0.190 0.09525" pos="0.3048-0.0125 0 0.0025+0.09525" rgba="1 .5 .5 1"/> -->
        <geom type="box" size="0.0125 0.190 0.09525" pos="0.2923 0 0.09775" rgba=".5 .5 .5 1"/>
        <!-- Front -->
        <!-- <box size="${side_wall_thickness} ${y_length} ${z_length}"/> -->
        <!-- <origin xyz="${x_length - side_wall_thickness/2} ${y_length/2} ${z_length/2}"/> -->
        <!-- <geom type="box" size="0.0125 0.190 0.09525" pos="-0.3048+0.0125 0 0.0025+0.09525" rgba="1 .5 .5 1"/> -->
        <geom type="box" size="0.0125 0.190 0.09525" pos="-0.2923 0 0.09775" rgba=".5 .5 .5 1"/>
        <!-- Right -->
        <!-- <box size="${x_length} ${side_wall_thickness} ${z_length}"/> -->
        <!-- <origin xyz="${x_length/2} ${side_wall_thickness/2} ${z_length/2}"/> -->
        <!-- <geom type="box" size="0.3048 0.0125 0.09525" pos="0 0.1905-0.0125 0.09775" rgba=".5 .5 .5 1"/> -->
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 0.178 0.09775" rgba=".5 .5 .5 1"/>
        <!-- Left -->
        <!-- <box size="${x_length} ${side_wall_thickness} ${z_length}"/> -->
        <!-- <origin xyz="${x_length/2} ${y_length - side_wall_thickness/2} ${z_length/2}"/> -->
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 -0.178 0.09775" rgba=".5 .5 .5 1"/>
      </body>

      <body name="right_bin" pos="0.25 0 0.025" quat="0.707 0 0 0.707">
        <geom type="box" size="0.3048 0.1905 0.0025" pos="0 0 0.0025" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.0125 0.190 0.09525" pos="0.2923 0 0.09775" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.0125 0.190 0.09525" pos="-0.2923 0 0.09775" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 0.178 0.09775" rgba=".5 .5 .5 1"/>
        <geom type="box" size="0.3048 0.0125 0.09525" pos="0 -0.178 0.09775" rgba=".5 .5 .5 1"/>
      </body>
    </body>
    
    <replicate count="4" offset=".07 0 0">
      <replicate count="7" offset="0 .07 0">
        <replicate count="2" offset="0 0 .14">
          <body pos="-0.35 0.8 0.5">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom size="0.025" condim="1" priority="1" rgba="0.8 0.2 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <replicate count="3" offset=".07 0 0">
      <replicate count="6" offset="0 .07 0">
        <replicate count="2" offset="0 0 .14">
          <body pos="-0.3 0.85 0.5">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom size="0.025" condim="1" priority="1" rgba="0.8 0.2 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Add a scene camera -->
    <site
      name="scene_camera_optical_frame"
      pos="0.0 1.0 1.6"
      euler="2.6415 0 3.1415"
    />
    <camera
      name="scene_camera"
      pos="0.0 1.0 1.6"
      fovy="58"
      mode="fixed"
      resolution="640 480"
      euler="-0.5 0 3.1415"
    />
    <body name="arm_pedestal" pos="0.0 0.3 0.15">
      <geom type="box" size="0.2 0.2 0.15" />
      <geom type="box" size="0.06 0.01 0.01" pos="0.23 0.03 0.1" />
      <geom type="box" size="0.06 0.01 0.01" pos="0.23 -0.03 0.1" />
      <site name="tool_holder_site" pos="0.25 0 0.15" euler="3.1415 0 0" />
    </body>
    <body name="arm_mount" pos="0.0 0.3 0.3">
      <include file="lrmate200id/lrmate200id.xml" />
    </body>
  </worldbody>

  <!-- Define weld constraints between the robot flange (tool0) and the gripper, and between the suction cup (tool_tip)
       and the blocks in the scene -->
  <equality>
  <!-- TODO -->
  </equality>
</mujoco>
