<mujoco model="kr16">
  <compiler angle="radian" autolimits="true" />
  <option integrator="implicit" />

  <default>
    <default class="kuka_actuator">
      <general ctrlrange="-6.28319 6.28319" />
      <position dampratio="1.0" />
    </default>
  </default>

  <asset>
    <mesh name="base_link" file="assets/base_link_collision.stl" />
    <mesh name="link_1" file="assets/link_1_collision.stl" />
    <mesh name="link_2" file="assets/link_2_collision.stl" />
    <mesh name="link_3" file="assets/link_3_collision.stl" />
    <mesh name="link_4" file="assets/link_4_collision.stl" />
    <mesh name="link_5" file="assets/link_5_collision.stl" />
    <mesh name="link_6" file="assets/link_6_collision.stl" />
    <mesh name="base_link_visual" file="assets/base_link_visual.stl" />
    <mesh name="link_1_visual" file="assets/link_1_visual.stl" />
    <mesh name="link_2_visual" file="assets/link_2_visual.stl" />
    <mesh name="link_3_visual" file="assets/link_3_visual.stl" />
    <mesh name="link_4_visual" file="assets/link_4_visual.stl" />
    <mesh name="link_5_visual" file="assets/link_5_visual.stl" />
    <mesh name="link_6_visual" file="assets/link_6_visual.stl" />

    <texture
      type="skybox"
      builtin="gradient"
      rgb1="0.3 0.5 0.7"
      rgb2="0 0 0"
      width="512"
      height="3072"
    />
    <material
      name="groundplane"
      texture="groundplane"
      texrepeat="1 1"
      reflectance="0.2"
    />
    <texture
      name="groundplane"
      type="2d"
      builtin="checker"
      width="512"
      height="512"
      rgb1="0.2 0.3 0.4"
      rgb2="0.1 0.2 0.3"
    />
  </asset>

  <worldbody>
    <!-- camera -->
    <camera
      name="scene_camera"
      pos="2 2 4"
      fovy="58"
      mode="fixed"
      resolution="640 480"
      xyaxes="-0.7071 0.7071 0
                 -0.577  -0.577 0.577"
    />
    <site
      name="scene_camera_optical_frame"
      pos="2 2 4"
      xyaxes="-0.7071 0.7071 0
                 -0.577  -0.577 0.577"
    />
    <!-- floor -->
    <geom
      name="floor"
      pos="0 0 0"
      size="0 0 0.05"
      type="plane"
      material="groundplane"
    />
    <body name="base_link" gravcomp="1" pos="0 0 0" quat="1 0 0 0">
      <inertial mass="1.0" pos="0 0 0" diaginertia="1 1 1" />
      <geom type="mesh" mesh="base_link" rgba="0 0 0 0" />
      <geom
        type="mesh"
        mesh="base_link_visual"
        contype="0"
        conaffinity="0"
        mass="0.0"
      />
      <body name="link_1" quat="0 -1 0 0" gravcomp="1">
        <inertial mass="1.0" pos="0 0 0" diaginertia="1 1 1" />
        <joint name="joint_1" pos="0 0 0" axis="0 0 1" limited="false" />
        <geom type="mesh" mesh="link_1" rgba="0 0 0 0" />
        <geom
          type="mesh"
          mesh="link_1_visual"
          contype="0"
          conaffinity="0"
          mass="0.0"
        />
        <body
          name="link_2"
          pos="0.16 0 -0.52"
          quat="0.707107 0.707107 0 0"
          gravcomp="1"
        >
          <inertial mass="1.0" pos="0 0 0" diaginertia="1 1 1" />
          <joint
            name="joint_2"
            pos="0 0 0"
            axis="0 0 1"
            range="-3.22886 1.13446"
            actuatorfrcrange="-1633.63 1633.63"
          />
          <geom type="mesh" mesh="link_2" rgba="0 0 0 0" />
          <geom
            type="mesh"
            mesh="link_2_visual"
            contype="0"
            conaffinity="0"
            mass="0.0"
          />
          <body
            name="link_3"
            pos="0.98 0 0"
            quat="0.707107 0 0 -0.707107"
            gravcomp="1"
          >
            <inertial mass="1.0" pos="0 0 0" diaginertia="1 1 1" />
            <joint
              name="joint_3"
              pos="0 0 0"
              axis="0 0 1"
              range="-2.40855 3.05433"
              actuatorfrcrange="-850.58 850.58"
            />
            <geom type="mesh" mesh="link_3" rgba="0 0 0 0" />
            <geom
              type="mesh"
              mesh="link_3_visual"
              contype="0"
              conaffinity="0"
              mass="0.0"
            />
            <body
              name="link_4"
              pos="0.15 0 0"
              quat="0.707107 0.707107 0 0"
              gravcomp="1"
            >
              <inertial mass="1.0" pos="0 0 0" diaginertia="1 1 1" />
              <joint
                name="joint_4"
                pos="0 0 0"
                axis="0 0 1"
                range="-6.10865 6.10865"
                actuatorfrcrange="-266.21 266.21"
              />
              <geom type="mesh" mesh="link_4" rgba="0 0 0 0" />
              <geom
                type="mesh"
                mesh="link_4_visual"
                contype="0"
                conaffinity="0"
                mass="0.0"
              />
              <body
                name="link_5"
                pos="0 0 -0.86"
                quat="0.707107 -0.707107 0 0"
                gravcomp="1"
              >
                <inertial mass="0.1" pos="0 0 0" diaginertia="0.01 0.01 0.01" />
                <joint
                  name="joint_5"
                  pos="0 0 0"
                  axis="0 0 1"
                  range="-2.26893 2.26893"
                  actuatorfrcrange="-255.438 255.438"
                />
                <geom type="mesh" mesh="link_5" rgba="0 0 0 0" />
                <geom
                  type="mesh"
                  mesh="link_5_visual"
                  contype="0"
                  conaffinity="0"
                  mass="0.0"
                />
                <body name="link_6" quat="0.707107 0.707107 0 0" gravcomp="1">
                  <inertial
                    mass="0.1"
                    pos="0 0 0"
                    diaginertia="0.01 0.01 0.01"
                  />
                  <joint
                    name="joint_6"
                    pos="0 0 0"
                    axis="0 0 1"
                    range="-6.10865 6.10865"
                    actuatorfrcrange="-111.658 111.658"
                  />
                  <geom type="mesh" mesh="link_6" rgba="0 0 0 0" />
                  <geom
                    type="mesh"
                    mesh="link_6_visual"
                    contype="0"
                    conaffinity="0"
                    mass="0.0"
                  />
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>
  <contact>
    <exclude body1="base_link" body2="link_1" />
    <exclude body1="link_1" body2="link_2" />
    <exclude body1="link_2" body2="link_3" />
    <exclude body1="link_3" body2="link_4" />
    <exclude body1="link_4" body2="link_5" />
    <exclude body1="link_5" body2="link_6" />
  </contact>
  <actuator>
    <position class="kuka_actuator" name="joint_1" joint="joint_1" kp="1000" />
    <position class="kuka_actuator" name="joint_2" joint="joint_2" kp="5000" />
    <position class="kuka_actuator" name="joint_3" joint="joint_3" kp="500" />
    <position class="kuka_actuator" name="joint_4" joint="joint_4" kp="100" />
    <position class="kuka_actuator" name="joint_5" joint="joint_5" kp="100" />
    <position class="kuka_actuator" name="joint_6" joint="joint_6" kp="100" />
  </actuator>

  <keyframe>
    <key
      name="default"
      qpos="0 -1.5708 1.5708 0 0 0"
      qvel="0 0 0 0 0 0"
      ctrl="0 -1.5708 1.5708 0 0 0"
    />
  </keyframe>
</mujoco>
