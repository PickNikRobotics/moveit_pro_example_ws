  <keyframe>
    <key
      name="home"
      qpos="-0.982958 -0.596154 -0.180735 1.4674 -0.188922 1.74071 -1.20581 0.784714 0.000368712 0.782839 -0.762532 0.784714 0.000371053 0.782824 -0.7626 -4.62558e-11 0.379998 0.031095 1 0 0 0"
