<mujoco model="bin_picking scene">
<!-- =============================================== -->

    <compiler angle="degree" coordinate="local"/>
    <asset>
        <material name="bin_material" rgba="0.984 0.353 0.290 1.0"/>
        <material name="framing_material" rgba="0.7 0.7 0.7 1"/>
        <texture name="floor_texture" type="2d" file="bin_picking/diamond_plate.png" />
        <texture name="vancouver-canucks-texture" type="2d" file="bin_picking/vancouver-canucks-logo.png" />
        <texture name="ottawa-senators-texture" type="2d" file="bin_picking/ottawa-senators-logo.png" />
        <texture name="toronto-maple-leafs-texture" type="2d" file="bin_picking/toronto-maple-leafs-logo.png" />
        <texture name="colorado-avalanche-texture" type="2d" file="bin_picking/colorado-avalanche-logo.png" />
        <texture name="calgary-flames-texture" type="2d" file="bin_picking/calgary-flames-logo.png" />
        <texture name="montreal-canadiens-texture" type="2d" file="bin_picking/montreal-canadiens-logo.png" />
        <texture name="edmonton-oilers-texture" type="2d" file="bin_picking/edmonton-oilers-logo.png" />
        <texture name="winnipeg-jets-texture" type="2d" file="bin_picking/winnipeg-jets-logo.png" />
        
        <!-- Define material using the checker texture -->
        <material name="floor_material" texture="floor_texture" texrepeat="10 10" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="vancouver-canucks-material" texture="vancouver-canucks-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="ottawa-senators-material" texture="ottawa-senators-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="toronto-maple-leafs-material" texture="toronto-maple-leafs-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="colorado-avalanche-material" texture="colorado-avalanche-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="calgary-flames-material" texture="calgary-flames-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="montreal-canadiens-material" texture="montreal-canadiens-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="edmonton-oilers-material" texture="edmonton-oilers-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>
        <material name="winnipeg-jets-material" texture="winnipeg-jets-texture" rgba="0.6 0.6 0.6 1" shininess="0.3" specular="0.3"/>

        <material name="shelf_material" rgba="0.6 0.4 0.2 1"/>
        <material name="cardboard_material" rgba="0.8 0.6 0.4 1"/>
    </asset>


<!-- =============================================== -->

<option timestep="0.001" integrator="implicitfast" gravity="0 0 -9.8">
  	<flag contact="enable" energy="enable"  multiccd="enable"/>
</option>
  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" directional="true" />
    <geom
      name="floor"
      pos="0 0 -0.5"
      size="0 0 0.05"
      type="plane"
      material="floor_material"
       contype="1" conaffinity="1" friction="0.5 0.3 0.1" solref="0.02 1" solimp="0.8 0.9 0.001" 
    />
    <!-- Scene Camera -->
    <site
      name="scene_camera_optical_frame"
      pos="-1.0 -2.0 2.966"
      euler="3.99 0.000 0"
    />
    <camera
      name="scene_camera"
      pos="-1.0 -2.0 2.966"
      fovy="58"
      mode="fixed"
      resolution="640 480"
      euler="0.849 0.000 0.0"
    />

<!-- ================================================================ -->









