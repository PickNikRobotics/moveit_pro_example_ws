        <geom type="cylinder" size="0.02 0.005"  material="winnipeg-jets-material" contype="1" conaffinity="1" mass="0.17" friction="0.5 0.3 0.1" solref="0.02 1" solimp="0.8 0.9 0.001" margin="0.0001"/>
