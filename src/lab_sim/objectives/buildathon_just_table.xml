<root BTCPP_format="4" main_tree_to_execute="Buildathon CropBox">
    <!--//////////-->
    <BehaviorTree ID="Buildathon CropBox" _description="" _favorite="true" _subtreeOnly="false">
        <Control ID="Sequence" name="TopLevelSequence">
            <SubTree ID="Clear Snapshot" _collapsed="true"/>
            <Action ID="GetPointCloud" topic_name="/wrist_camera/points"/>
            <Action ID="CreateStampedPose" stamped_pose="{eef_pose}"/>
            <Action ID="TransformPoseFrame" target_frame_id="wrist_camera_optical_frame" input_pose="{eef_pose}" output_pose="{eef_pose}"/>
            <Action ID="TransformPose" input_pose="{eef_pose}" translation_xyz="1;0;0" output_pose="{crop_pose}"/>
            <Action ID="CropPointsInBox" crop_box_centroid_pose="{crop_pose}" crop_box_size="2;1.5;2"/>
            <Action ID="SendPointCloudToUI" point_cloud="{point_cloud_cropped}"/>
        </Control>
    </BehaviorTree>
    <TreeNodesModel>
        <SubTree ID="Buildathon"/>
    </TreeNodesModel>
</root>