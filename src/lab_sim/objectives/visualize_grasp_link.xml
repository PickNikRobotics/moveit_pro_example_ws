<?xml version="1.0" encoding="UTF-8" ?>
<root BTCPP_format="4" main_tree_to_execute="Visualize Grasp Link">
  <!--//////////-->
  <BehaviorTree ID="Visualize Grasp Link" _description="" _favorite="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <SubTree
        ID="Get Transform Frame Pose"
        _collapsed="true"
        requested_pose="{grasp_pose}"
        requested_frame="grasp_link"
      />
      <Action
        ID="VisualizePose"
        marker_lifetime="0.000000"
        marker_name="pose"
        marker_size="0.100000"
        pose="{grasp_pose}"
      />
      <Action
        ID="VisualizeMesh"
        marker_lifetime="0.000000"
        mesh_name="mesh"
        mesh_path="package://lab_sim/objectives/gripper_token.dae"
        mesh_pose="{grasp_pose}"
      />
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Visualize Grasp Link">
      <MetadataFields>
        <Metadata runnable="true" />
        <Metadata subcategory="Application - Advanced Examples" />
      </MetadataFields>
    </SubTree>
  </TreeNodesModel>
</root>
