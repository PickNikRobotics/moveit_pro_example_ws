<root BTCPP_format="4" main_tree_to_execute="Buildathon croppedPointCloud">
    <!--//////////-->
    <BehaviorTree ID="Buildathon croppedPointCloud" _description="" _favorite="true" _subtreeOnly="false">
        <Control ID="Sequence" name="TopLevelSequence">
            <Action ID="CreateStampedPose" stamped_pose="{crop_pose}" reference_frame="wrist_camera_optical_frame" position_xyz="0;0;.5"/>
            <Action ID="VisualizePose" pose="{crop_pose}" marker_name="crop_pose"/>
            <Action ID="CropPointsInBox" crop_box_centroid_pose="{crop_pose}" crop_box_size="0.1;0.1;2" point_cloud="{point_cloud_noTable}"/>
            <Action ID="SendPointCloudToUI" point_cloud="{point_cloud_cropped}"/>
        </Control>
    </BehaviorTree>
    <TreeNodesModel>
        <SubTree ID="Buildathon"/>
    </TreeNodesModel>
</root>