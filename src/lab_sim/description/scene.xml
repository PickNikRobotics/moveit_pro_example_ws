<!-- UR5e inspired from https://github.com/google-deepmind/mujoco_menagerie/tree/main/universal_robots_ur5e -->
<mujoco model="ur5e scene">
  <include file="ur5e/ur5e_linear_rail_globals.xml" />
  <include file="lab_desk/desk_globals.xml" />
  <include file="robotiq_2f85/robotiq2f85_globals.xml" />

  <compiler angle="radian" autolimits="true" />

  <option integrator="implicitfast" impratio="10" noslip_iterations="3" />

  <!-- nuser_cam=1 enables per-camera user params: 0=RGB+Depth, 1=RGB-only, 2=3D-lidar -->
  <size nuser_cam="1" />

  <asset>
    <!-- Laptop keyboard texture (dark checker pattern) -->
    <texture
      name="keyboard_tex"
      type="2d"
      builtin="checker"
      rgb1="0.03 0.03 0.03"
      rgb2="0.08 0.08 0.08"
      width="256"
      height="256"
    />
    <material
      name="keyboard_mat"
      texture="keyboard_tex"
      texrepeat="10 6"
      specular="0.2"
      shininess="0.3"
    />
    <!-- Laptop screen texture (dark IDE-like gradient) -->
    <texture
      name="screen_tex"
      type="2d"
      builtin="gradient"
      rgb1="0.05 0.12 0.18"
      rgb2="0.02 0.06 0.10"
      width="256"
      height="256"
    />
    <material name="screen_mat" texture="screen_tex" emission="0.4" />
    <texture
      name="desktop_screenshot_tex"
      type="cube"
      filedown="laptop-screen.png"
      rgb1="0.02 0.02 0.02"
    />
    <material
      name="desktop_screenshot_mat"
      texture="desktop_screenshot_tex"
      emission="0.3"
    />
    <!-- Keyboard texture -->
    <texture
      name="keyboard_img_tex"
      type="cube"
      filefront="keyboard-texture.png"
      rgb1="0.15 0.15 0.15"
    />
    <material
      name="keyboard_img_mat"
      texture="keyboard_img_tex"
      specular="0.2"
      shininess="0.3"
    />
    <!-- AprilTag material for tray is provided by desk_globals.xml (apriltag_material1) -->
    <!-- Red plastic tray material -->
    <material
      name="red_plastic"
      rgba="0.75 0.15 0.1 1"
      specular="0.6"
      shininess="0.7"
    />
    <!-- Light brown plastic pot material -->
    <material
      name="pot_plastic"
      rgba="0.72 0.52 0.32 1"
      specular="0.5"
      shininess="0.6"
    />
  </asset>

  <!-- Set offscreen rendering resolution to match camera sensor resolution (640x480) so that
       MuJoCo renders images at the correct size for the simulated wrist and scene cameras. -->
  <visual>
    <global offwidth="640" offheight="480" />
    <headlight ambient="0.3 0.3 0.3" diffuse="0.5 0.5 0.5" />
  </visual>

