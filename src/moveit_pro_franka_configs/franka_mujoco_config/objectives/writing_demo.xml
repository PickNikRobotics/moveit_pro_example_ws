      <SubTree ID="Move to Waypoint" _collapsed="true" acceleration_scale_factor="0.1" controller_action_server="/joint_trajectory_controller/follow_joint_trajectory" controller_names="/joint_trajectory_controller" joint_group_name="arm" keep_orientation="false" keep_orientation_tolerance="0.05" link_padding="0.01" velocity_scale_factor="0.1" waypoint_name="Home" seed="0"/>
