      <Control ID="Sequence">
        <Action ID="CreateStampedPose" reference_frame="fr3_link0" stamped_pose="{stamped_pose}" orientation_xyzw="-0.7073883;0.7068252;0.0000327;0.0000328" position_xyz="0.6;0.3;0.09"/>
        <Action ID="VisualizePose" marker_lifetime="0.000000" marker_name="pose" marker_size="0.100000" pose="{stamped_pose}"/>
